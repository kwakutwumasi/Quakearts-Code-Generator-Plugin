<?xml version="1.0" encoding="UTF-8"?>
<scaffolding id="hibernate-app" xmlns="http://quakearts.com/scaffolding"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://quakearts.com/scaffolding ../model/scaffolding.xsd ">
	<properties>
		<entry property="apptitle">Database Application</entry>
		<entry property="company">Quake Arts Inc.</entry>
		<entry property="jndiname">CRUDDB</entry>
		<entry property="datasourcetype">sqlserver</entry>
		<entry property="serverName">localhost</entry>
		<entry property="databaseName">master</entry>
		<entry property="user">sa</entry>
		<entry property="password">12345</entry>
		<entry property="serverPort"></entry>
		<entry property="createrole">CreateRole</entry>
		<entry property="deleterole">DeleteRole</entry>
		<entry property="readrole">ReadRole</entry>
		<entry property="updaterole">UpdateRole</entry>
		<entry property="loginmodule"><![CDATA[
			<login-module code="" flag="required">
				<module-option name=""></module-option>
				<module-option name=""></module-option>
				<module-option name=""></module-option>
			</login-module>
		]]></entry>
	</properties>
	<template-group folderID="tm">
		<template filename="crud.xhtml" location="crudapp/crud.xhtml" />
		<template filename="create.xhtml" location="crudappsecure/create.xhtml" />
		<template filename="list.xhtml" location="crudappsecure/list.xhtml" />
	</template-group>
	<resource folderID="images" location="crudapp/images/loading.gif" />
	<resource folderID="images" location="crudapp/images/loading-small.gif" />
	<resource folderID="beans" location="crudapp/crudappapplicationbean.vm" filename="CRUDApplicationBean.java" template="true" />
	<resource folderID="web" location="crudapp/index.vm" filename="index.jsp" template="true" />
	<resource folderID="webinf" location="crudappsecure/web.xml" template="true" filename="web.xml" />
	<resource folderID="webinf" location="crudapp/app-xa-ds.xml" template="true" />
	<resource folderID="webinf" location="crudappsecure/crudapp.login-jboss-beans.xml" template="true" />
	<resource folderID="webinf" location="crudapp/faces-config.xml" />
	<resource folderID="base" location="crudappsecure/template.xhtml" template="true" />
	<resource folderID="ui" location="crudapp/main.xhtml" template="true" />
	<folder-structure>
		<folder name="Root Folder" folderID="root">
			<folder name="src" folderID="src">
				<folder folderID="com" name="com">
					<folder folderID="quakearts" name="quakearts">
						<folder folderID="webapp" name="webapp">
							<folder folderID="beans" name="beans" />
						</folder>
					</folder>
				</folder>
			</folder>
			<folder name="WebContent" folderID="web">
				<folder name="base" folderID="base" />
				<folder name="resources" folderID="resources">
					<folder name="images" folderID="images" />
				</folder>
				<folder name="WEB-INF" folderID="webinf">
					<folder name="classes" folderID="classes" />
				</folder>
				<folder name="ui" folderID="ui" />
				<folder name="tm" folderID="tm" />
			</folder>
		</folder>
	</folder-structure>
</scaffolding>
